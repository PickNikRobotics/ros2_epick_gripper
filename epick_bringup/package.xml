<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>epick_bringup</name>
  <version>0.0.1</version>
  <description>
    Bringup package for the Robotiq EPick gripper for testing purposes.
  </description>
  <maintainer email="giovanni.remigi@picknik.ai">Giovanni Remigi</maintainer>
  <license>BSD</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <depend>epick_description</depend>
  <depend>epick_driver</depend>
  <depend>epick_controllers</depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
